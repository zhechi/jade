.touxiang
  width 100px
  height 100px
  margin 0 auto
  position relative
  img
    display block
    width 100px
    height 100px
    border-radius 100%
  span
    position absolute
    display block
    width 24px
    height 24px
    border-radius 100%
    background rgba(0,159,237,.5)
