.form_center
    margin 16px 46px
    font-size 14px
    color rgb(98,98,98)
    .address
      width 544px
      display inline-block
      em
        vertical-align top
        padding-top 10px
        display inline-block
      
    .form-item-provinces-td
      margin-left 2px
    label
      width 64px
      display inline-block
      text-align right
      vertical-align: top;
      padding-top: 10px;
    fieldset
      margin-bottom 24px
      p
        text-align right
        padding-right 62px
        padding-top 8px
      .count
        border none
        display inline
        width auto
        margin 0
        padding 0
        text-align right
      
    input
    textarea
      display inline-block
      width 524px
      border 1px solid #959595
      line-height 32px
      padding 0 8px
      margin 0 5px 0 2px
      color rgb(98,98,98)
    span
      color rgb(255,32,50)
    textarea
      line-height 18px
      height 40px
      padding 8px
      font-size 14px
      vertical-align -webkit-baseline-middle
    .error
      border 1px solid rgb(255,32,50)
    .success
      border 1px solid blue_color
    .form-item
      display inline-block
      margin-bottom 7px
      select
        width 100%
        height 32px
        padding-right 12px 
        padding-left: 8px;
        background-color: transparent;
        -webkit-appearance: none;
    .form-select-overlay
      display inline-block
      border 1px solid rgb(150,150,150) 
      width 169px
      height 32px 
      background url(../img/down.png) no-repeat 155px center
